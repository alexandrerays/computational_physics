#include <stdio.h>
#include<conio.h>

void ler_vetor(int v[],int n)
{
 int i; 
 for(i=0;i<n;i++)
   {
   printf("Elemento %d: ",i+1);
   scanf("%d",&v[i]);   
   }
}

void imprimir_vetor(int v[], int n)
{
  int i=0;
  for (i=0;i<n;i++)    
  printf ("%d ", v[i]);
}

int verifica(int v[],int x,int N)
{
    int i;
    for(i=0;i<N; i++);
     if(v[i]==x)
     return (1);
     else
     return(0);
}

void inserir (int v[],int x,int p);

void remover(int v[],int pos);

int somar(int v [])
{
 int s,i;
 for(i=0;i<20;i++);
      s=s+v[i];
 return (s);
}


int max(int v[])
{
   int max=0,i;
    for(i=1;i<2;i++)
    {
     if(v[i]>max)
      max=v[i];
      }
      return (max);    
    
}

main()
{
 int a[20];
 int P,E,N;
 
 //ler elementos do vetor
  
  puts ("entre com o numero de elementos");
  scanf("%d",&N);
  
  ler_vetor(a,N);
  imprimir_vetor (a,N);
  getch();
  
  
  /*printf("Insira o numero a ser verificado:");
